CreateTransaction.js","original":{"line":53,"column":21}},{"generated":{"line":98,"column":0},"source":"components/CreateTransaction.js","original":{"line":51,"column":9}},{"generated":{"line":98,"column":14},"source":"components/CreateTransaction.js","original":{"line":28,"column":6}},{"generated":{"line":98,"column":15},"source":"components/CreateTransaction.js","original":{"line":27,"column":3}},{"generated":{"line":99,"column":0},"source":"components/CreateTransaction.js","original":{"line":56,"column":1}},{"name":"CreateTransaction","generated":{"line":101,"column":15},"source":"components/CreateTransaction.js","original":{"line":58,"column":15}}],"sources":{"components/CreateTransaction.js":"import React, {useState} from 'react';\nimport {Link,useHistory} from 'react-router-dom';\n\nfunction CreateTransaction() {\n   let history=useHistory();\n   const [recipient,setRecipient] = useState('');\n   const [amount,setAmount] = useState(0);\n   const updateRecipient = e => {\n      setRecipient(e.target.value);\n   }\n\n   const updateAmount = e => {\n      setAmount(parseInt(e.target.value));\n   }\n\n   const createTransaction = ()=> {\n      fetch('http://localhost:3000/api/transact', {\n         method:'POST',\n         headers: {'Content-Type':'application/json'},\n         body: JSON.stringify({recipient,amount})\n      }).then(response=>response.json())\n         .then(json=>{\n            alert(json.message || json.type)\n            history.push('/transaction-pool');\n         })\n   }\n   return (\n      <div className=\"conductTransaction\">\n         <button type=\"button\" className=\"btn btn-outline-success\">\n            <Link to='/'><h2>Home</h2></Link>\n         </button>\n         <h3>Create Transaction</h3>\n         <div className=\"form-group\">\n            <label htmlFor=\"recipient\">Enter Recipient Address</label>\n            <input type=\"text\"\n               id=\"recipient\"\n               placeholder=\"recipient\"\n               value={recipient}\n               onChange={updateRecipient}\n               className=\"form-control\"\n            />\n            <label htmlFor=\"amount\">Enter Amount</label>\n            <input type=\"number\"\n               id=\"amount\"\n               placeholder=\"Amount\"\n               value={amount}\n               onChange={updateAmount}\n               className=\"form-control\"\n            />\n         </div>\n         <button type=\"submit\"\n            className=\"btn btn-danger\"\n            onClick={createTransaction}>Submit</button>\n      </div>\n   )\n}\n\nexport default CreateTransaction\n"},"lineCount":null}},"error":null,"hash":"57503dc02ee256c0c3431e5ee172234e","cacheData":{"env":{}}}