ransactionPool.js","original":{"line":59,"column":12}},{"generated":{"line":86,"column":13},"source":"components/TransactionPool.js","original":{"line":59,"column":21}},{"generated":{"line":86,"column":15},"source":"components/TransactionPool.js","original":{"line":59,"column":22}},{"generated":{"line":87,"column":0},"source":"components/TransactionPool.js","original":{"line":57,"column":9}},{"generated":{"line":87,"column":29},"source":"components/TransactionPool.js","original":{"line":39,"column":6}},{"generated":{"line":87,"column":30},"source":"components/TransactionPool.js","original":{"line":38,"column":3}},{"generated":{"line":88,"column":0},"source":"components/TransactionPool.js","original":{"line":65,"column":1}},{"name":"TransactionPool","generated":{"line":90,"column":15},"source":"components/TransactionPool.js","original":{"line":68,"column":15}}],"sources":{"components/TransactionPool.js":"import React, {useState,useEffect} from 'react';\nimport Transaction from './Transaction';\nimport {Link,useHistory} from 'react-router-dom';\n\nconst POLL_INTERVAL_MS = 10000;\n\nfunction TransactionPool() {\n   const [transactionPoolMap,setTransactionPoolMap] = useState({});\n   const history=useHistory();\n   const fetchTransactionPoolMap = ()=> {\n      fetch('http://localhost:3000/api/transaction-pool-map')\n         .then(response=>response.json())\n         .then(data=>setTransactionPoolMap(data));\n   }\n\n   const fetchMineTransactions = ()=> {\n      fetch(`${document.location.origin}/api/mine-transactions`)\n         .then(response=> {\n            if(response.status===200){\n               alert('success');\n               history.push('/blocks');\n            } else {\n               alert('The mine transactions block request did not complete.')\n            }\n         });\n   }\n\n   useEffect(()=>{\n      fetchTransactionPoolMap();\n      const fetchPoolMapInterval = setInterval(()=> fetchTransactionPoolMap(),POLL_INTERVAL_MS)\n\n      return () => {\n         clearInterval(fetchPoolMapInterval);\n      }\n   },[]);\n\n   \n   return (\n      <div className='TransactionPool'>\n         <button type=\"button\" className=\"btn btn-outline-success\">\n            <Link to='/'><h2>Home</h2></Link>\n         </button>\n         <br />\n         <br />\n         <h3>Transaction Pool </h3>\n         {\n            Object.values(transactionPoolMap).map(transaction => {\n               return (\n                  <div key={transaction.id}>\n                     <hr />\n                     <Transaction transaction={transaction} />\n                  </div>\n               )\n            })\n         }\n         <hr />\n         <button type=\"button\" \n            onClick = {fetchMineTransactions}\n            className=\"btn btn-danger\">\n            Mine the Transactions\n         </button>\n         \n      </div>\n   )\n}\n\n\nexport default TransactionPool\n"},"lineCount":null}},"error":null,"hash":"be82adbd6619157b8f513a2a6335bd1d","cacheData":{"env":{}}}